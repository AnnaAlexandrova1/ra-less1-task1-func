{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "linebreak-style": 0,
      "class-methods-use-this": 0,
      "no-useless-escape": 0,
      "no-empty": 0,
      "no-unused-vars": 0,
      "no-shadow": 0,
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }